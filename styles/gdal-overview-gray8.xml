<Map>
    <Parameters>
        <Parameter name="sizes">256,256</Parameter>
        <Parameter name="bbox">0,0,512,512</Parameter>
    </Parameters>

    <Style name="raster">
        <Rule>
            <RasterSymbolizer>
                <stop color="black" value="0"/>
                <stop color="white" value="255"/>
            </RasterSymbolizer>
        </Rule>
    </Style>

    <Layer name="dataraster">
        <StyleName>raster</StyleName>
        <Datasource>
            <Parameter name="file">../../data/tiff/overview-test-gray8.tif</Parameter>
            <Parameter name="type">gdal</Parameter>
            <Parameter name="band">1</Parameter>
        </Datasource>
    </Layer>
</Map>
