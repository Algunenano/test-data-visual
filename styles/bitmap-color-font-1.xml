<Map font-directory="../fonts/Funkster/">
    <Parameters>
        <Parameter name="sizes">256, 256</Parameter>
        <Parameter name="bbox">-1, -1, 1, 1</Parameter>
        <Parameter name="cairo">off</Parameter>
    </Parameters>
    <Style name="1">
        <Rule>
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                size="46"
                halo-radius="3"
                halo-fill="red"
                halo-rasterizer="fast"
                >
                "Test: "
                <Format face-name="Funkster Medium">"Test"</Format>
            </TextSymbolizer>
            <DebugSymbolizer />
        </Rule>
    </Style>
    <Layer name="1">
        <StyleName>1</StyleName>
        <Datasource>
            <Parameter name="type">csv</Parameter>
            <Parameter name="inline">
                x, y
                0, 0
            </Parameter>
        </Datasource>
    </Layer>
</Map>
